{
	let
		Int1 ~ 10_00;
		Int2 : Integer;
	in
	{
		if (Int1 > 999)
		unless (Int1 < 99)
			Putint(in Int1 * 10)
		else
			Putint(in Int1)
		;
		
		Int2 := Int1 - 990;
		
		while (Int2 > 10)
		do {
			Putint(in Int2);
			Int2 := Int2 - 1;
		};
	};

	with 
		WithVar ~ 10
	do
	{
		Putint(in WithVar);
	};
	done
	;
}